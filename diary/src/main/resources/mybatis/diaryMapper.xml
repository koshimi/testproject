<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.example.demo.Mapper.DiaryMapper">
	<insert id="insertDiary">
		INSERT INTO diary (null, #{user_id}, #{title}, #{content}, SYSTDATE())
	</insert>
	<select id="getDiary" resultType="Member">
		SELECT #{board_id},#{user_id},#{title},#{content},#{create_at}
		FROM diary 
		WHERE user_id = #{user_id}
	</select>
</mapper>